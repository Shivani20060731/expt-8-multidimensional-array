#include <iostream>
using namespace std;
int main() {
    int n;
    cout << "Enter the size of the matrix: ";
    cin >> n;
    int matrix[n][n];
    cout << "Enter matrix elements:\n";
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cin >> matrix[i][j];
        }
    }
    
    cout << "Transpose of the matrix is:\n";
    for (int j = 0; j < n; j++) {
        for (int i = 0; i < n; i++) {
            cout << matrix[i][j] << " ";  // Swap rows and columns
        }
        cout << endl;
    }
    return 0;
}
